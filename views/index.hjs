<!DOCTYPE html>
<html>
  <head>
  	<meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    <title>{{{ title }}}</title>        
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
    <link rel='stylesheet' href='/stylesheets/bootstrap-theme.min.css' />
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/dashboard.css' />
    <script src='/javascript/jquery.min.js'></script>
    <script src='/javascript/bootstrap.min.js'></script>
    <script src='/javascript/gtech.js'></script>
    
  </head>
  <body>
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">GTech Auto Care</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li><a href="#">Dashboard</a></li>
            <li><a href="#">Settings</a></li>
            <li><a href="#">Profile</a></li>
            <li><a href="#">Help</a></li>
          </ul>
          <form class="navbar-form navbar-right">
            <input type="text" class="form-control" placeholder="Search...">
          </form>
        </div>
      </div>
    </div>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 col-md-2 sidebar">
          <ul class="nav nav-sidebar">
            <li class="active"><a href="#">Overview</a></li>
            <li><a href="#">Reports</a></li>
            <li><a href="#">Analytics</a></li>
            <li><a href="#">Export</a></li>
          </ul>          
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">

          <!-- Nav tabs -->
          <ul id="mainTab" class="nav nav-tabs">
            <li class="active"><a href="#job" data-toggle="tab">Job</a></li>
            <li><a href="#manufacturer" data-toggle="tab">Manufacturer</a></li>
            <li><a href="#model" data-toggle="tab">Model</a></li>
            <li><a href="#servicetype" data-toggle="tab">Service Type</a></li>            
          </ul>

          <!-- Tab panes -->
          <div class="tab-content">

            <!-- Manufacturers tab-->
            <div class="tab-pane" id="manufacturer">
              <h2 class="sub-header">Manufacturers</h2>
              <div class="table-responsive">
                <form role="newManufacturerForm" method="post" action="/manufacturer/save">                
                  {{#manufacturer}}                    
                    <input type="hidden" class="form-control" id="id" name="id" value={{id}}>
                  {{/manufacturer}}
                  {{^manufacturer}}
                    <input type="hidden" class="form-control" id="id" name="id" value="-1"}>
                  {{/manufacturer}}
                  <div class="form-group">                    
                    <label for="name">Name</label>
                    {{#manufacturer}}                    
                    	<input type="text" class="form-control" id="name" name="name" placeholder="Name" value="{{{name}}}">
                    {{/manufacturer}}
                    {{^manufacturer}}
                    	<input type="text" class="form-control" id="name" name="name" placeholder="Name">
                    {{/manufacturer}}
                  </div>
                  <div class="form-group">
                    <label for="description">Description</label>
                    {{#manufacturer}}                    
                    	<input type="input" class="form-control" id="description" name="description" placeholder="Description" value="{{{description}}}">
                    {{/manufacturer}}
                    {{^manufacturer}}
                    	<input type="input" class="form-control" id="description" name="description" placeholder="Description">
                    {{/manufacturer}}                    
                  </div>                  
                  <button type="submit" class="btn btn-default">Save</button>                  
                </form> 
              </div>
              <hr>
              <div class="table-responsive">
                <table class="table table-striped">
                  <thead>                
                    <tr>
                      <th>Name</th>
                      <th>Description</th>                      
                    </tr>
                  </thead>
                  <tbody>
                    {{#manufacturers}}
                      <tr>
                        <td><a href='/manufacturer/{{id}}'>{{{name}}}</a></td>
                        <td>{{{description}}}</td>                        
                      </tr>
                    {{/manufacturers}}
                  </tbody>
                </table>
              </div>     
            </div>

            <!-- Models tab-->
            <div class="tab-pane" id="model">
              <h2 class="sub-header">Models</h2>                
              <div class="table-responsive">
                <form role="newModelForm" method="post" action="/model/save">
                  {{#model}}                    
                    <input type="hidden" class="form-control" id="id" name="id" value={{id}}>
                  {{/model}}
                  {{^model}}
                    <input type="hidden" class="form-control" id="id" name="id" value="-1"}>
                  {{/model}}
                  <div class="form-group">
                    <label for="make">Make</label>
                    <select class="form-control" id="make" name="make" placeholder="make">
                      {{#manufacturers}}
                      <option>{{name}}</option>
                      {{/manufacturers}}                      
                    </select>                    
                  </div>
                  <div class="form-group">
                    <label for="name">Name</label>
                    {{#model}}
                      <input type="text" class="form-control" id="name" name="name" placeholder="Name" value="{{{name}}}">
                    {{/model}}
                    {{^model}}
                      <input type="text" class="form-control" id="name" name="name" placeholder="Name">
                    {{/model}}                    
                  </div>
                  <div class="form-group">
                    <label for="description">Description</label>
                    {{#model}}                    
                      <input type="input" class="form-control" id="description" name="description" placeholder="Description" value="{{{description}}}">
                    {{/model}}
                    {{^model}}
                      <input type="input" class="form-control" id="description" name="description" placeholder="Description">
                    {{/model}}                    
                  </div>                  
                  <button type="submit" class="btn btn-default">Save</button>
                </form> 
              </div>
              <hr>
              <div class="table-responsive">
                <table class="table table-striped">
                  <thead>                
                    <tr>
                      <th>Make</th>
                      <th>Name</th>
                      <th>Description</th>                      
                    </tr>
                  </thead>
                  <tbody>
                    {{#models}}
                      <tr>
                        <td>{{{make}}}</td>
                        <td><a href='/model/{{id}}'>{{{name}}}</a></td>
                        <td>{{{description}}}</td>                        
                      </tr>
                    {{/models}}
                  </tbody>
                </table>
              </div>      
            </div>

            <!-- Services tab-->
            <div class="tab-pane" id="servicetype">
              <h2 class="sub-header">Service Types</h2>
              <div class="table-responsive">
                <form role="newServiceTypeForm" method="post" action="/servicetype/save">
                  {{#servicetype}}                    
                    <input type="hidden" class="form-control" id="id" name="id" value={{id}}>
                  {{/servicetype}}
                  {{^servicetype}}
                    <input type="hidden" class="form-control" id="id" name="id" value="-1"}>
                  {{/servicetype}}
                  <div class="form-group">
                    <label for="code">Code</label>
                    {{#servicetype}}
                      <input type="text" class="form-control" id="code" name="code" placeholder="Code" value="{{{code}}}">
                    {{/servicetype}}
                    {{^servicetype}}
                      <input type="text" class="form-control" id="code" name="code" placeholder="Code">
                    {{/servicetype}}                    
                  </div>
                  <div class="form-group">
                    <label for="name">Name</label>
                    {{#servicetype}}
                      <input type="text" class="form-control" id="name" name="name" placeholder="Name" value="{{{name}}}">
                    {{/servicetype}}
                    {{^servicetype}}
                      <input type="text" class="form-control" id="name" name="name" placeholder="Name">
                    {{/servicetype}}                    
                  </div>
                  <div class="form-group">
                    <label for="description">Description</label>
                    {{#servicetype}}
                      <input type="input" class="form-control" id="description" name="description" placeholder="Description" value="{{{description}}}">
                    {{/servicetype}}
                    {{^servicetype}}
                      <input type="input" class="form-control" id="description" name="description" placeholder="Description">
                    {{/servicetype}}                    
                  </div>                  
                  <button type="submit" class="btn btn-default">Save</button>
                </form> 
              </div>
              <hr>
              <div class="table-responsive">
                <table class="table table-striped">
                  <thead>                
                    <tr>
                      <th>Code</th>
                      <th>Name</th>
                      <th>Description</th>                      
                    </tr>
                  </thead>
                  <tbody>
                    {{#servicetypes}}
                      <tr>
                        <td>{{{code}}}</td>
                        <td><a href='/servicetype/{{id}}'>{{{name}}}</a></td>
                        <td>{{{description}}}</td>                        
                      </tr>
                    {{/servicetypes}}
                  </tbody>
                </table>
              </div>     
            </div>

            <!-- Jobs tab-->
            <div class="tab-pane active" id="job">              
  	          <h2 class="sub-header">Jobs</h2>          
  	          <div class="table-responsive">
                <form role="newJobForm" method="post" action="/job/save">
                  <div class="col-xs-6">   
                      {{#job}}                    
                        <input type="hidden" class="form-control" id="id" name="id" value={{id}}>
                      {{/job}}
                      {{^job}}
                        <input type="hidden" class="form-control" id="id" name="id" value="-1"}>
                      {{/job}}
                      <div class="form-group">
                        <label for="make">Manufacturer</label>
                        <select class="form-control" id="make" name="make" placeholder="Manufacturer">
                          {{#manufacturers}}
                          <option>{{name}}</option>
                          {{/manufacturers}}                      
                        </select>                                            
                      </div>
                      <div class="form-group">
                        <label for="model">Model</label>
                        <select class="form-control" id="model" name="model" placeholder="Model">
                          {{#models}}
                          <option>{{name}}</option>
                          {{/models}}                      
                        </select>                                            
                      </div>
                      <div class="form-group">
                        <label for="rego">Rego</label>
                        {{#job}}                    
                          <input type="input" class="form-control" id="rego" name="rego" placeholder="Rego" value="{{{rego}}}">
                        {{/job}}
                        {{^job}}
                          <input type="input" class="form-control" id="rego" name="rego" placeholder="Rego">
                        {{/job}}
                      </div>
                      <div class="form-group">
                        <label for="name">Name</label>
                        {{#job}}                    
                          <input type="input" class="form-control" id="name" name="name" placeholder="Name" value="{{{name}}}">
                        {{/job}}
                        {{^job}}
                          <input type="input" class="form-control" id="name" name="name" placeholder="Name">
                        {{/job}}
                      </div>
                      <div class="form-group">
                        <label for="contact">Contact</label>
                        {{#job}}                    
                          <input type="input" class="form-control" id="contact" name="contact" placeholder="Contact" value="{{{contact}}}">
                        {{/job}}
                        {{^job}}
                          <input type="input" class="form-control" id="contact" name="contact" placeholder="Contact">
                        {{/job}}
                      </div>
                      <button type="submit" class="btn btn-default">Save Job</button>                  
                  </div>
                  <div class="col-xs-6">
                    <div class="form-group">
                      <label for="servicetype">Service Type</label>
                      <select multiple class="form-control" id="servicetype" name="servicetype" placeholder="Service Type" style="height:173px">
                        {{#serviceTypes}}
                        <option>{{name}}</option>
                        {{/serviceTypes}}                      
                      </select>                       
                    </div>
                    <div class="form-group">
                      <label for="note">Notes</label>        
                      {{#job}}                    
                        <textarea class="form-control" id="note" name="note" placeholder="Note" style="height:104px" value="{{{note}}}"></textarea>
                      {{/job}}
                      {{^job}}
                        <textarea class="form-control" id="note" name="note" placeholder="Note" style="height:104px"></textarea>
                      {{/job}}
                    </div>
                  </div>  	          	
                </form> 
  	          </div>
  	          <hr>
  	          <div class="table-responsive">
  	            <table class="table table-striped">
  	              <thead>                
  	                <tr>
  	                  <th>Rego</th>
                      <th>Make</th>
                      <th>Model</th>
  	                  <th>Name</th>
  	                  <th>Contact</th>  	                  
  	                  <th>Service</th>
                      <th>Status</th>                      
  	                </tr>
  	              </thead>
  	              <tbody>
  	                {{#jobs}}
  	                  <tr>
  	                    <td><a href='/job/{{id}}'>{{{rego}}}</a></td>
                        <td>{{{make}}}</td>
                        <td>{{{model}}}</td>
  	                    <td>{{{name}}}</td>
  	                    <td>{{{contact}}}</td>
  	                    <td>{{{servicetypes}}}</td>                        
  	                    <td><input type="checkbox" name="status_1001" value="1"></td>
  	                  </tr>
  	                {{/jobs}}
  	              </tbody>
  	            </table>
  	          </div>	            
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>